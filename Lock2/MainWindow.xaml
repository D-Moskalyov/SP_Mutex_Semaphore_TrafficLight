<Window x:Class="Lock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="387" Width="136" Closing="Window_Closing">
    <Grid>
        <Ellipse Name="Green" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="62" Margin="31,29,0,0" Stroke="Black" VerticalAlignment="Top" Width="62"/>
        <Ellipse Name="Yellow" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="62" Margin="31,122,0,0" Stroke="Black" VerticalAlignment="Top" Width="62"/>
        <Ellipse Name="Red" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="62" Margin="31,215,0,0" Stroke="Black" VerticalAlignment="Top" Width="62"/>
        <Button Content="Start" HorizontalAlignment="Left" Margin="31,298,0,0" VerticalAlignment="Top" Width="62" Click="Button_Click"/>
        <Button Content="Stop" HorizontalAlignment="Left" Margin="31,323,0,0" VerticalAlignment="Top" Width="62" Click="Button_Click_1"/>

    </Grid>
</Window>
